{
  "name": "@z22092/event-polyfill",
  "description": "polyfill event",
  "readmeFilename": "README.md",
  "keywords": [
    "event",
    "polyfill"
  ],
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "license": "MIT",
  "version": "0.0.5",
  "scripts": {
    "start": "node dist",
    "dev": "ts-node-dev --transpile-only --respawn --project tsconfig.json src/index.ts",
    "dev:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json src/index.ts",
    "build": "rimraf dist && tsx ./build.ts",
    "release": "standard-version",
    "prerelease": "yarn build",
    "lint": "eslint --ext .ts ./",
    "lint:fix": "eslint --fix  --ext .ts  ./src",
    "doc": "typedoc --tsconfig tsconfig/docs.json",
    "doc:gen": "yarn doc:clean && yarn doc",
    "doc:clean": "rimraf docs",
    "doc:config": "yarn doc --showConfig"
  },
  "author": {
    "name": "Jefferson Venceslau",
    "email": "jeff.venceslau@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z22092/"
  },
  "engines": {
    "node": ">=14.21.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.16.12",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-define-config": "^1.13.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^3.12.1",
    "typedoc": "^0.23.24",
    "typedoc-plugin-coverage": "^2.0.0",
    "typedoc-plugin-extras": "^2.3.2",
    "typedoc-plugin-mdn-links": "^2.0.2",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {}
}
