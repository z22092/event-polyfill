{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "target": "ES6",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
  },
  "include": [
    "../../src/**/*.ts",
    "../../src/**/*.d.ts"
  ],
  "exclude": [
    "../../src/**/*.spec.ts",
    "../../node_modules",
    "../../build.ts",
    "../../*.js"
  ]
}
